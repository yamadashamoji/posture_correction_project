<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.posture.app.controller.MainController"
      spacing="10">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    
    <HBox spacing="10">
        <Button fx:id="inputButton" text="入力"/>
        <Button fx:id="calendarButton" text="カレンダー"/>
        <Button fx:id="graphButton" text="グラフ"/>
        <Button fx:id="settingsButton" text="設定"/>
    </HBox>
    
    <VBox fx:id="mainContainer" VBox.vgrow="ALWAYS"/>
</VBox>
